# Variables d'environnement pour le frontend (client-side)
# Doivent être préfixées par VITE_ pour être accessibles dans le code React

# Connexion Supabase (non-sensible, publique)
VITE_SUPABASE_URL=https://your-project-url.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# URL publique du site
VITE_PUBLIC_SITE_URL=localhost

# Autres variables publiques si nécessaire
VITE_RAPIDAPI_KEY=your_rapidapi_key_here
VITE_RAPIDAPI_HOST=your_rapidapi_host_here
